    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dbg: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "dbg: put_service",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/services/put/put_service.py",
            "console": "integratedTerminal"
        },
        {
            "name": "dbg: ta_service",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/services/ta/ta_service.py",
            "console": "integratedTerminal",
            "args": [
                "-ta_name",
                "macd"
            ]
        }
    ]
}